        "#include <maya/MFnPlugin.h>"
    ],

    "fn_initPlug" : [
        "MStatus initializePlugin( MObject obj )",
        "{",
            "\tMStatus status;",
            "",
            "\tMFnPlugin plugin( obj, \"%_#_VENDOR_#_%\", \"%_#_VERSION_#_%\", \"%_#_REQAPIVER_#_%\");",
            "",
            "\t// Register the translator with the system",
            "\tstatus =  plugin.registerFileTranslator(\"%_#_FTNAME_#_%\", \"none\", %_#_FTCLASSNAME_#_%::creator,\"%_#_UISCRIPTNAME_#_%\");",
            "",
            "\tif (status != MS::kSuccess)",
            "\t{",
                    "\t\tMGlobal::displayInfo(\"Error loading File Translator\");",
                    "\t\tstatus.perror(\"Maya::registerFileTranslator\");",
            "\t}",
            "\treturn status;",
        "}"
    ],

    "fn_uninitPlug" : [
        "MStatus uninitializePlugin(MObject obj)",
        "{",
            "\tMFnPlugin plugin(obj);",
            "\tMStatus status =  plugin.deregisterFileTranslator(\"%_#_FTNAME_#_%\");",
            "\tif (status != MS::kSuccess)",
            "\t{",
                "\t\tMGlobal::displayInfo(\"Error removing File Translator\");",
                "\t\tstatus.perror(\"Maya::deregisterFileTranslator\");",
            "\t}",
            "",
            "\treturn status;",
        "}"
    ]
