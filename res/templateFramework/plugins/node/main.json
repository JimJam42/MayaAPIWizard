    "includes" : [
        "#include \"%_#_NODEINCLUDE_#_%\"",
        "#include <maya/MGlobal.h>",
        "#include <maya/MStatus.h>",
        "#include <maya/MObject.h>",
        "#include <maya/MFnPlugin.h>"
    ],

    "fn_initPlug" : [
        "MStatus initializePlugin( MObject obj )",
        "{",
            "\tMStatus status;",
            "",
            "\tMFnPlugin plugin( obj, \"%_#_VENDOR_#_%\", \"%_#_VERSION_#_%\", \"%_#_REQAPIVER_#_%\");",
            "",
            "\t// Register the node with the system",
            "\tstatus =  plugin.registerNode(%_#_CLASSNAME_#_%::m_typeName,%_#_CLASSNAME_#_%::m_typeId,%_#_CLASSNAME_#_%::creator,%_#_CLASSNAME_#_%::initialize,%_#_MPXNODETYPE_#_%);",
            "",
            "\tif (status != MS::kSuccess)",
            "\t{",
                    "\t\tMGlobal::displayInfo(\"Error registering the Node\");",
                    "\t\tstatus.perror(\"Maya::registerNode\");",
            "\t}",
            "\treturn status;",
        "}"
    ],

    "fn_uninitPlug" : [
        "MStatus uninitializePlugin(MObject obj)",
        "{",
            "\tMFnPlugin plugin(obj);",
            "\tMStatus status =  plugin.deregisterNode(%_#_CLASSNAME_#_%::m_typeId);;",
            "\tif (status != MS::kSuccess)",
            "\t{",
                "\t\tMGlobal::displayInfo(\"Error removing Node\");",
                "\t\tstatus.perror(\"Maya::deregisterNode\");",
            "\t}",
            "",
            "\treturn status;",
        "}"
    ]
}
